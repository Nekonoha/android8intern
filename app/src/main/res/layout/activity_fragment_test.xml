<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.nekonoha.youtubeapp.FragmentTestActivity">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="468dp"
        android:gravity="center_horizontal|fill_horizontal"
        android:orientation="vertical"
        android:weightSum="1"
        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="4dp">


        <fragment
            android:id="@+id/fragment"
            android:name="com.example.nekonoha.youtubeapp.SearchFragment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0.32"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_editor_absoluteY="8dp" />

        <fragment
            android:id="@+id/img_frag"
            android:name="com.example.nekonoha.youtubeapp.ImageFragment"
            android:layout_width="match_parent"
            android:layout_height="117dp"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="113dp" />

    </LinearLayout>
</FrameLayout>

